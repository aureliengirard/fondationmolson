<!--[if gt IE 8]><!--><html class="no-js" {{ site.language_attributes }}> <!--<![endif]-->
<head>
    <meta charset="{{ site.charset }}" />
    <title>
        {% if wp_title %}
            {{ wp_title }} - {{ site.name }}
        {% else %}
            {{ site.name }}
        {% endif %}
    </title>
    <meta name="description" content="{{ site.description }}">
    <link rel="stylesheet" href="{{ site.theme.link }}/style.css" type="text/css" media="screen" />
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="author" href="{{ site.theme.link }}/humans.txt" />
    <link rel="pingback" href="{{ site.pingback_url }}" />

    {{function('wp_head')}}

    <link rel="stylesheet" href="{{ site.theme.link }}/static/helpers/css/animate.css" type="text/css" media="all">
    <link rel="stylesheet" href="{{ site.theme.link }}/static/helpers/css/bootstrap.min.css" type="text/css" media="all">
    <link rel="stylesheet" href="{{ site.theme.link }}/static/helpers/css/et-line-icons.css" type="text/css" media="all">
    <link rel="stylesheet" href="{{ site.theme.link }}/static/helpers/css/font-awesome.min.css" type="text/css" media="all">
    <link rel="stylesheet" href="{{ site.theme.link }}/static/helpers/css/themify-icons.css" type="text/css" media="all">
    <link rel="stylesheet" href="{{ site.theme.link }}/static/helpers/css/swiper.min.css" type="text/css" media="all">
    <link rel="stylesheet" href="{{ site.theme.link }}/static/helpers/css/justified-gallery.min.css" type="text/css" media="all">
    <link rel="stylesheet" href="{{ site.theme.link }}/static/helpers/css/magnific-popup.css" type="text/css" media="all">

    <link rel="stylesheet" href="{{ site.theme.link }}/static/helpers/css/bootsnav.css" type="text/css" media="all">
    <link rel="stylesheet" href="{{ site.theme.link }}/static/helpers/css/theme.css" type="text/css" media="all">
    <link rel="stylesheet" href="{{ site.theme.link }}/static/helpers/css/responsive.css" type="text/css" media="all">
    <link rel="stylesheet" href="{{ site.theme.link }}/static/helpers/css/sass/style.css" type="text/css" media="all">

    <!--[if IE]>
